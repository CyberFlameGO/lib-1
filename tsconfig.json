{
  "extends": "@younho9/tsconfig/tsconfig.json",
  "compilerOptions": {
    "allowJs": true
  },
  "exclude": [
    "packages/babel-preset",
    "packages/prettier-config",
    "packages/tsconfig",
    "**/dist/**"
  ],
  "typedocOptions": {
    "readme": "none",
    "out": "doc",
    "entryPointStrategy": "packages",
    "entryPoints": ["packages/guards", "packages/object", "packages/types"]
  }
}
