{
  "name": "@younho9/eslint-plugin",
  "version": "0.1.4",
  "homepage": "https://github.com/younho9/lib#readme",
  "bugs": {
    "url": "https://github.com/younho9/lib/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/younho9/lib.git",
    "directory": "packages/eslint-plugin"
  },
  "license": "MIT",
  "author": "Younho Choo <younho9.choo@gmail.com>",
  "exports": {
    ".": {
      "require": "./dist/cjs/index.js",
      "default": "./dist/esm/index.js"
    },
    "./constants": {
      "require": "./dist/cjs/constants/index.js",
      "default": "./dist/esm/constants/index.js"
    }
  },
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/types/index.d.ts",
  "typesVersions": {
    "*": {
      "constants": [
        "./dist/types/constants"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "prepublish": "yarn build",
    "build": "concurrently yarn:build:*",
    "build:cjs": "cross-env BABEL_ENV=cjs babel src --extensions .ts,.tsx -d dist/cjs --source-maps",
    "build:esm": "cross-env BABEL_ENV=esm babel src --extensions .ts,.tsx -d dist/esm --source-maps",
    "build:types": "tsc --emitDeclarationOnly --declaration --declarationDir dist/types",
    "lint": "concurrently yarn:lint:*",
    "lint:src": "eslint src --ext .ts,.tsx",
    "lint:types": "tsc --noEmit"
  },
  "devDependencies": {
    "@types/eslint": "7.28.0",
    "@typescript-eslint/eslint-plugin": "4.31.1",
    "@typescript-eslint/parser": "4.31.1",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-node": "0.3.6",
    "eslint-plugin-import": "2.24.2",
    "eslint-plugin-jsx-a11y": "6.4.1",
    "eslint-plugin-prettier": "4.0.0",
    "eslint-plugin-react": "7.25.1",
    "eslint-plugin-react-hooks": "4.2.0",
    "prettier": "2.4.0"
  },
  "peerDependencies": {
    "@typescript-eslint/eslint-plugin": ">=4",
    "@typescript-eslint/parser": ">=4",
    "eslint": ">=0.8.0",
    "eslint-config-prettier": ">=8",
    "eslint-import-resolver-node": ">=0",
    "eslint-plugin-import": ">=2",
    "eslint-plugin-jsx-a11y": ">=6",
    "eslint-plugin-prettier": ">=4",
    "eslint-plugin-react": ">=7",
    "eslint-plugin-react-hooks": ">=4",
    "prettier": "*"
  },
  "publishConfig": {
    "access": "public"
  }
}
