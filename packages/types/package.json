{
  "name": "@younho9/types",
  "version": "0.5.0",
  "homepage": "https://github.com/younho9/lib#readme",
  "bugs": {
    "url": "https://github.com/younho9/lib/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/younho9/lib.git",
    "directory": "packages/types"
  },
  "license": "MIT",
  "author": "Younho Choo <younho9.choo@gmail.com>",
  "sideEffects": false,
  "type": "module",
  "main": "dist/types/index.d.ts",
  "types": "dist/types/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "prepublish": "yarn build",
    "prebuild": "yarn clean:dist",
    "build": "concurrently yarn:build:*",
    "build:types": "tsc src/*.ts --emitDeclarationOnly --declaration --declarationDir dist/types",
    "clean": "concurrently yarn:clean:*",
    "clean:dist": "rimraf dist",
    "lint": "concurrently yarn:lint:*",
    "lint:types": "tsc --noEmit",
    "test": "concurrently yarn:test:*",
    "test:types": "tsd"
  },
  "devDependencies": {
    "@younho9/eslint-plugin": "0.1.6"
  },
  "publishConfig": {
    "access": "public"
  },
  "tsd": {
    "directory": "__tests__"
  }
}
